{
    "name": "evabee/schema-org-json-ld",
    "description": "library for generating json-ld rich results",
	"keywords": [
		"schema.org",
		"json-ld",
		"structured data",
		"rich results",
		"google",
		"seo",
		"product",
		"article",
		"event",
		"recipe",
		"faq",
		"job posting",
		"local business",
		"video"
	],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "EvaLok\\SchemaOrgJsonLd\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Eva Beekers",
            "email": "eva.aedea@gmail.com"
        }
    ],
    "require": {
		"php" : ">=8.1.0"
	},
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^2.0"
    },
	"scripts": {
		"test-unit": "phpunit --bootstrap vendor/autoload.php test/unit",
		"cs-check": "php-cs-fixer fix --dry-run --diff",
		"cs-fix": "php-cs-fixer fix",
		"phpstan": "phpstan analyse"
	}
}
